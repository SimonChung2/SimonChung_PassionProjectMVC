@model SimonChung_PassionProject.Models.Dealer
@{
    ViewBag.Title = "Edit";
}

<h2>Edit Dealer </h2>
<form action="/dealer/update/@Model.DealerID" method="POST">
    <div style="display:none;">
        @* Pass through the animal ID *@
        <input type="text" name="DealerID" value="@Model.DealerID" />
    </div>
    <div>
        <label>Dealer Name:</label><input type="text" name="DealerName" value="@Model.DealerName" />
    </div>

    <div>
        <input type="submit" value="Edit Dealer" />
    </div>
</form>

